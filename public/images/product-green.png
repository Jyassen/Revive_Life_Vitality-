<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="300" height="400" viewBox="0 0 300 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Gradient for the bottle -->
  <linearGradient id="bottleGradient" x1="0%" y1="0%" x2="100%" y2="100%">
    <stop offset="0%" stop-color="#4caf50" />
    <stop offset="100%" stop-color="#388e3c" />
  </linearGradient>
  
  <!-- Bottle shape -->
  <g transform="translate(100, 40)">
    <!-- Bottle cap -->
    <rect x="35" y="0" width="30" height="20" rx="5" fill="#333" />
    
    <!-- Bottle neck -->
    <rect x="40" y="20" width="20" height="30" fill="#4caf50" />
    
    <!-- Main bottle body -->
    <path d="M30,50 h40 a40,40 0 0 1 40,40 v160 a40,40 0 0 1 -40,40 h-40 a40,40 0 0 1 -40,-40 v-160 a40,40 0 0 1 40,-40 z" 
          fill="url(#bottleGradient)" />
    
    <!-- Green veggies icons -->
    <g transform="translate(50, 130)">
      <!-- Kale leaf -->
      <path d="M-15,-20 c-10,-5 -20,5 -10,15 c10,10 20,5 15,-5 c-5,-10 5,-15 10,-5 c5,10 -5,15 -15,5" 
            fill="#1b5e20" stroke="#fff" stroke-width="1" />
      
      <!-- Spinach leaf -->
      <path d="M15,-15 c10,-5 20,5 10,15 c-10,10 -20,5 -15,-5 c5,-10 -5,-15 -10,-5 c-5,10 5,15 15,5" 
            fill="#2e7d32" stroke="#fff" stroke-width="1" />
      
      <!-- Cucumber slice -->
      <circle cx="0" cy="15" r="12" fill="#a5d6a7" />
      <circle cx="0" cy="15" r="8" fill="#81c784" />
      <circle cx="0" cy="15" r="4" fill="#4caf50" />
      <circle cx="-3" cy="12" r="1" fill="#a5d6a7" />
      <circle cx="2" cy="18" r="1" fill="#a5d6a7" />
      <circle cx="4" cy="13" r="1" fill="#a5d6a7" />
    </g>
    
    <!-- Product label -->
    <rect x="20" y="170" width="60" height="80" rx="5" fill="#fff" fill-opacity="0.8" />
    
    <!-- Product name -->
    <text x="50" y="190" font-family="Arial, sans-serif" font-size="9" font-weight="bold" text-anchor="middle" fill="#333">GREEN</text>
    <text x="50" y="203" font-family="Arial, sans-serif" font-size="9" font-weight="bold" text-anchor="middle" fill="#333">DETOX</text>
    
    <!-- Volume -->
    <text x="50" y="225" font-family="Arial, sans-serif" font-size="8" text-anchor="middle" fill="#333">250ml</text>
    
    <!-- Extra details -->
    <text x="50" y="240" font-family="Arial, sans-serif" font-size="6" text-anchor="middle" fill="#666">CLEANSE FORMULA</text>
  </g>
</svg> 