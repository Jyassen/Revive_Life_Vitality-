<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="300" height="400" viewBox="0 0 300 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Gradient for the bottle -->
  <linearGradient id="bottleGradient" x1="0%" y1="0%" x2="100%" y2="100%">
    <stop offset="0%" stop-color="#f5b93f" />
    <stop offset="100%" stop-color="#d48e1a" />
  </linearGradient>
  
  <!-- Bottle shape -->
  <g transform="translate(100, 40)">
    <!-- Bottle cap -->
    <rect x="35" y="0" width="30" height="20" rx="5" fill="#333" />
    
    <!-- Bottle neck -->
    <rect x="40" y="20" width="20" height="30" fill="#f5b93f" />
    
    <!-- Main bottle body -->
    <path d="M30,50 h40 a40,40 0 0 1 40,40 v160 a40,40 0 0 1 -40,40 h-40 a40,40 0 0 1 -40,-40 v-160 a40,40 0 0 1 40,-40 z" 
          fill="url(#bottleGradient)" />
    
    <!-- Honey icon -->
    <g transform="translate(50, 130)">
      <path d="M-20,-25 h40 v50 h-40 z" fill="#e8a01b" />
      <path d="M-20,-25 h40 v10 h-40 z" fill="#f5b93f" />
      <path d="M-10,0 c15,-10 25,10 10,15 c-15,5 -25,-5 -10,-15" fill="#ffe0a3" />
      
      <!-- Honey dipper -->
      <line x1="0" y1="-30" x2="0" y2="-5" stroke="#8b4513" stroke-width="3" />
      <path d="M-8,-5 h16 v-3 h-16 z" fill="#8b4513" />
      <path d="M-6,-8 h12 v-3 h-12 z" fill="#8b4513" />
      <path d="M-4,-11 h8 v-3 h-8 z" fill="#8b4513" />
    </g>
    
    <!-- Product label -->
    <rect x="20" y="170" width="60" height="80" rx="5" fill="#fff" fill-opacity="0.8" />
    
    <!-- Product name -->
    <text x="50" y="190" font-family="Arial, sans-serif" font-size="9" font-weight="bold" text-anchor="middle" fill="#333">MANUKA</text>
    <text x="50" y="203" font-family="Arial, sans-serif" font-size="9" font-weight="bold" text-anchor="middle" fill="#333">HONEY</text>
    <text x="50" y="216" font-family="Arial, sans-serif" font-size="9" font-weight="bold" text-anchor="middle" fill="#333">BOOST</text>
    
    <!-- Volume -->
    <text x="50" y="235" font-family="Arial, sans-serif" font-size="8" text-anchor="middle" fill="#333">250ml</text>
    
    <!-- Extra details -->
    <text x="50" y="250" font-family="Arial, sans-serif" font-size="6" text-anchor="middle" fill="#666">IMMUNITY SUPPORT</text>
  </g>
</svg> 